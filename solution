#include <bits/stdc++.h>
using namespace std;
#define tc() int t; cin>>t; while(t--)
#define loop(i,a,b) for(int i=a;i<b;i++)

int main() {
    int count=0;
	tc(){
	    string s;
	    cin>>s;
	    int a=s.size();
	    loop(i,0,a-1){
	        if(((s[i]=='c')&&(s[i+1]=='h'))||((s[i]=='h')&&(s[i+1]=='e'))||((s[i]=='e')&&(s[i+1]=='f'))){
	            count++;
	            break;
	        }
	    }
	}
	cout<<count<<'\n';
	return 0;
}
